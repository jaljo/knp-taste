{% extends "base.html.twig" %}

{% block body %}
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}
       
    {{ form_start(loginForm, {"action": path("login"), "method": "POST"}) }}
        {{ form_widget(loginForm) }}
        <button type="submit" name="_login">login</button>
    {{ form_end(loginForm) }}
{% endblock body %}